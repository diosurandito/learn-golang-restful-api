{
    "openapi":"3.0.2",
    "info": {
      "title":"Category RESTful API",
      "description": "API Spec for Category RESTful API",
      "version":"1.0"
    },
    "servers": [
      {"url":"http://localhost:3000/api"}
    ],
    "paths": {
        "/categories": {
            "get" : {
                "responses": {
                    "200": {
                        "description": "Succes get all categories",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "code": {
                                            "type": "number"
                                        },
                                        "status": {
                                            "type": "string"
                                        },
                                        "data" : {
                                            "type": "array",
                                            "items": {
                                                "type": "object",
                                                "properties": {
                                                    "id": {
                                                        "type": "number"
                                                    },
                                                    "name": {
                                                        "type": "string"
                                                    }
                                                }

                                            }
                                            
                                        }
                                    }
                                }
                            }
                        }

                    }
                }
            }
        }
        
    }
  }